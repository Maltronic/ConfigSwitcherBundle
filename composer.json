{
    "name": "maltronic/config-switcher-bundle",
    "type": "symfony-bundle",
    "description": "This bundle provides events for dynamically changing Symfony2 application config parameters",
    "keywords": ["Symfony2", "bundle", "doctrine"],
    "homepage": "https://github.com/maltronic/ConfigSwitcherBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Malcolm Davis",
            "email": "maltronic.email@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "~2.3",
        "symfony/framework-bundle": "~2.3"
    },
    "autoload": {
        "psr-0": { "Maltronic\\Bundle\\ConfigSwitchBundle": "" }
    },
    "target-dir": "Maltronic/Bundle/ConfigSwitcherBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~4.1"
    }
}

